/* https://leetcode.com/problems/plus-one/ */

class Solution {
    public int[] plusOne(int[] digits) {
        int lin=digits.length;
    for(int i=lin-1;i>=0;i--){
        if(digits[i]<9){
            digits[i]=digits[i]+1;
            return digits;
        }
        digits[i]=0;
    }
    int[] newnumber=new int[lin+1];
    newnumber[0]=1;
    return newnumber;


    }
    }
