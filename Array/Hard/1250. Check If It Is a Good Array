/* https://leetcode.com/problems/check-if-it-is-a-good-array/description/ */

class Solution {
    public boolean isGoodArray(int[] nums) {
        int ans=nums[0];
        int n=nums.length;
        for(int i=1;i<n;i++){
            ans=gcd(ans,nums[i]);
        }
        return ans==1;
    }
    static int gcd(int n,int m){
        if(m==0){
            return n;
        }
        else{

        return gcd(m,n%m);
        }
    }
}
