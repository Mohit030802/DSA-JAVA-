/* https://leetcode.com/problems/length-of-last-word/ */

class Solution {
    public int lengthOfLastWord(String s) {
       String se=s.trim();
       int count=0;
       for(int i=se.length()-1;i>=0;i--) {
           if(se.charAt(i)==' '){
               break;
           }
           else{
               count++;
           }
       }
       return count;
    }
}
