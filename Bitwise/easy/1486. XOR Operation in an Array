/*  https://leetcode.com/problems/xor-operation-in-an-array/ */


class Solution {
    public int xorOperation(int n, int start) {
        int[] newarr=new int[n];
        int xorsum=0;
        for(int i=0;i<newarr.length;i++){
            newarr[i]=start+2*i;
            xorsum^=newarr[i];

        }
        return xorsum;
    }
}
